  "bugs": {
    "url": "https://github.com/frontendphil/katjak/issues"
  },
  "homepage": "https://github.com/frontendphil/katjak#readme",
  "devDependencies": {
    "eslint": "^3.9.0",
    "eslint-config-signavio": "^1.3.2",
    "rimraf": "^2.5.4"
  },
  "engines": {
    "node": "7.0.0",
    "npm": "4.0.0"
  }
}
